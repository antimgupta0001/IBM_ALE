FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions [0 2 -2 0 0 0 0];

internalField uniform 0;

boundaryField
{
    ibCylinder
    {
 	 type mixedIb;
        patchType immersedBoundary;
        triValue uniform 0;
        triGradient uniform 0;
        triValueFraction uniform 0;

        setDeadValue   yes;
        deadValue      0;
    }
    in
    {
        type zeroGradient;
    }
    out
    {
        type fixedValue;
        value uniform 0;
    }
    top
    {
        type zeroGradient;
    }
    bottom
    {
        type zeroGradient;
    }
    frontAndBack
    {
        type empty;
    }
}


